<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<StackPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Scalar_Result_Controller">
   <children>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="600.0">
         <top>
            <AnchorPane prefHeight="149.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <children>
                  <Text fx:id="title" layoutX="213.0" layoutY="125.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Title Text">
                     <font>
                        <Font size="37.0" />
                     </font>
                  </Text>
               </children>
            </AnchorPane>
         </top>
         <left>
            <AnchorPane prefHeight="451.0" prefWidth="612.0" BorderPane.alignment="CENTER">
               <children>
                  <Text fx:id="operation_text" layoutX="41.0" layoutY="219.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Operation:">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Text>
                  <ScrollPane layoutX="300.0" layoutY="125.0" prefHeight="200.0" prefWidth="200.0">
                     <content>
                        <GridPane fx:id="entries">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                        </GridPane>
                     </content>
                  </ScrollPane>
                  <Button fx:id="operation_select" layoutX="204.0" layoutY="377.0" mnemonicParsing="false" onAction="#goBack" text="Return to Operation Select" />
               </children>
            </AnchorPane>
         </left>
      </BorderPane>
   </children>
</StackPane>
